{
  "provider": {
    "name": "simple_provider"
  },
  "consumer": {
    "name": "simple_consumer"
  },
  "interactions": [
    {
      "description": "a request to say Hello",
      "request": {
        "method": "GET",
        "path": "/client/simple"
      },
      "response": {
        "status": 200,
        "body": {
          "hello": "harry"
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.0.10"
    }
  }
}